<mvc:View xmlns:mvc="sap.ui.core.mvc" controllerName="com.public.storage.paoro.controller.Internet"                                                                                                                                                            
    xmlns="sap.uxap"                                                                                                                                                                                                                                           
    xmlns:m="sap.m"                                                                                                                                                                                                                                            
    xmlns:core="sap.ui.core"                                                                                                                                                                                                                                   
    xmlns:l="sap.ui.layout"                                                                                                                                                                                                                                    
    xmlns:f="sap.ui.layout.form" height="100%">                                                                                                                                                                                                                
                                                                                                                                                                                                                                                               
    <ObjectPageLayout id="ObjectPageLayout" upperCaseAnchorBar="false">                                                                                                                                                                                        
        <headerTitle>                                                                                                                                                                                                                                          
            <ObjectPageDynamicHeaderTitle>                                                                                                                                                                                                                     
                <expandedHeading>                                                                                                                                                                                                                              
                    <m:Title text="Internet Details" wrapping="true" />                                                                                                                                                                                        
                </expandedHeading>                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                               
                <snappedHeading>                                                                                                                                                                                                                               
                    <m:HBox>                                                                                                                                                                                                                                   
                        <m:VBox>                                                                                                                                                                                                                               
                            <m:Title text="Internet Details" wrapping="true"/>                                                                                                                                                                                 
                            <m:Label text="Fill your internet details"/>                                                                                                                                                                                       
                        </m:VBox>                                                                                                                                                                                                                              
                    </m:HBox>                                                                                                                                                                                                                                  
                </snappedHeading>                                                                                                                                                                                                                              
                                                                                                                                                                                                                                                               
                <expandedContent>                                                                                                                                                                                                                              
                    <m:Label text="Fill your internet details"/>                                                                                                                                                                                               
                </expandedContent>                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                               
                <snappedTitleOnMobile>                                                                                                                                                                                                                         
                    <m:Title text="Internet Details"/>                                                                                                                                                                                                         
                </snappedTitleOnMobile>                                                                                                                                                                                                                        
                                                                                                                                                                                                                                                               
            </ObjectPageDynamicHeaderTitle>                                                                                                                                                                                                                    
        </headerTitle>                                                                                                                                                                                                                                         
                                                                                                                                                                                                                                                               
        <headerContent>                                                                                                                                                                                                                                        
            <m:FlexBox wrap="Wrap" fitContainer="true">                                                                                                                                                                                                        
                <m:Avatar class="sapUiMediumMarginEnd sapUiSmallMarginBottom" src="sap-icon://internet-browser" displayShape="Circle" initials="ID" displaySize="L"/>                                                                                          
                                                                                                                                                                                                                                                               
                <m:VBox class="sapUiLargeMarginEnd sapUiSmallMarginBottom">                                                                                                                                                                                    
                    <m:Title level="H4" titleStyle="H4" text="Plant" class="sapUiTinyMarginBottom">                                                                                                                                                            
                    </m:Title>                                                                                                                                                                                                                                 
                    <m:Label text="{plantsModel>/plant}"/>                                                                                                                                                                                                     
                </m:VBox>                                                                                                                                                                                                                                      
                                                                                                                                                                                                                                                               
                <m:VBox class="sapUiSmallMarginBottom sapUiLargeMarginEnd">                                                                                                                                                                                    
                    <m:Title level="H4" titleStyle="H4" text="Property" class="sapUiTinyMarginBottom" />                                                                                                                                                       
                    <m:Label text="{plantsModel>/property}"/>                                                                                                                                                                                                  
                </m:VBox>                                                                                                                                                                                                                                      
                                                                                                                                                                                                                                                               
                <m:VBox class="sapUiSmallMarginBottom">                                                                                                                                                                                                        
                    <m:Title level="H4" titleStyle="H4" text="Address" class="sapUiTinyMarginBottom" />                                                                                                                                                        
                    <m:Label wrapping="true" text="{plantsModel>/address}"/>                                                                                                                                                                                   
                </m:VBox>                                                                                                                                                                                                                                      
            </m:FlexBox>                                                                                                                                                                                                                                       
        </headerContent>                                                                                                                                                                                                                                       
                                                                                                                                                                                                                                                               
        <sections>                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                               
            <ObjectPageSection titleUppercase="false" id="personalSection" title="Internet Details">                                                                                                                                                           
                <subSections>                                                                                                                                                                                                                                  
                    <ObjectPageSubSection id="personalSectionSS1" titleUppercase="false">                                                                                                                                                                      
                        <blocks>                                                                                                                                                                                                                               
                            <f:SimpleForm id="basicDetails" editable="true" layout="ResponsiveGridLayout"  labelSpanXL="4" labelSpanL="4" labelSpanM="12" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0" emptySpanL="0" emptySpanM="0" emptySpanS="0"+
 columnsXL="2" columnsL="2" columnsM="2" singleContainerFullSize="false">                                                                                                                                                                                      
                                <f:content>                                                                                                                                                                                                                    
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                               
                                    <m:Label  text="Network 1 IP address" />                                                                                                                                                                                   
                                    <m:Input enabled="false" id="net1IpAdd"  valueState="{/n1ip}"  value="{plantBasicDetailsModel>/Network1IpAddress}" />                                                                                                      
                                                                                                                                                                                                                                                               
                                    <m:Label  text="Network 2 IP address" />                                                                                                                                                                                   
                                    <m:Input enabled="false" id="net2IpAdd"  valueState="{/n2ip}"  value="{plantBasicDetailsModel>/Network2IpAddress}" />                                                                                                      
                                                                                                                                                                                                                                                               
                                    <m:Label text="Network Type" />                                                                                                                                                                                            
                                    <m:Input enabled="false" id="ntype"  value="{plantBasicDetailsModel>/NetworkType}" />                                                                                                                                      
                                                                                                                                                                                                                                                               
                                    <m:Label text="Network Vendor" />                                                                                                                                                                                          
                                    <m:Input enabled="false" id="nvendor"  value="{plantBasicDetailsModel>/NetworkVendor}" />                                                                                                                                  
                                    <m:Label text="Network Router Address" />                                                                                                                                                                                  
                                    <m:Input enabled="false" id="nrouteradd"  value="{plantBasicDetailsModel>/NetworkRouterAddress}" />                                                                                                                        
                                                                                                                                                                                                                                                               
                                </f:content>                                                                                                                                                                                                                   
                            </f:SimpleForm>                                                                                                                                                                                                                    
                                                                                                                                                                                                                                                               
                        </blocks>                                                                                                                                                                                                                              
                    </ObjectPageSubSection>                                                                                                                                                                                                                    
                </subSections>                                                                                                                                                                                                                                 
            </ObjectPageSection>                                                                                                                                                                                                                               
                                                                                                                                                                                                                                                               
        </sections>                                                                                                                                                                                                                                            
    </ObjectPageLayout>                                                                                                                                                                                                                                        
</mvc:View>                                                                                                                                                                                                                                                    