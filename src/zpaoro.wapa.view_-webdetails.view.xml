<mvc:View xmlns:mvc="sap.ui.core.mvc"                                                                                                                                                                                                                          
controllerName="com.public.storage.paoro.controller.WebDetails"                                                                                                                                                                                                
	xmlns="sap.uxap"                                                                                                                                                                                                                                              
	xmlns:m="sap.m"                                                                                                                                                                                                                                               
	xmlns:core="sap.ui.core"                                                                                                                                                                                                                                      
	xmlns:l="sap.ui.layout"                                                                                                                                                                                                                                       
	xmlns:f="sap.ui.layout.form" height="100%">                                                                                                                                                                                                                   
                                                                                                                                                                                                                                                               
	<ObjectPageLayout id="ObjectPageLayout" upperCaseAnchorBar="false">                                                                                                                                                                                           
		<headerTitle>                                                                                                                                                                                                                                                
			<ObjectPageDynamicHeaderTitle>                                                                                                                                                                                                                              
				<expandedHeading>                                                                                                                                                                                                                                          
					<m:Title text="Web Details" wrapping="true" />                                                                                                                                                                                                            
				</expandedHeading>                                                                                                                                                                                                                                         
                                                                                                                                                                                                                                                               
				<snappedHeading>                                                                                                                                                                                                                                           
					<m:HBox>                                                                                                                                                                                                                                                  
						<m:VBox>                                                                                                                                                                                                                                                 
							<m:Title text="Web Details" wrapping="true"/>                                                                                                                                                                                                           
							<m:Label text="Fill your web details"/>                                                                                                                                                                                                                 
						</m:VBox>                                                                                                                                                                                                                                                
					</m:HBox>                                                                                                                                                                                                                                                 
				</snappedHeading>                                                                                                                                                                                                                                          
                                                                                                                                                                                                                                                               
				<expandedContent>                                                                                                                                                                                                                                          
					<m:Label text="Fill your web details"/>                                                                                                                                                                                                                   
				</expandedContent>                                                                                                                                                                                                                                         
                                                                                                                                                                                                                                                               
				<snappedTitleOnMobile>                                                                                                                                                                                                                                     
					<m:Title text="Web Details"/>                                                                                                                                                                                                                             
				</snappedTitleOnMobile>                                                                                                                                                                                                                                    
                                                                                                                                                                                                                                                               
			                                                                                                                                                                                                                                                            
			</ObjectPageDynamicHeaderTitle>                                                                                                                                                                                                                             
		</headerTitle>                                                                                                                                                                                                                                               
                                                                                                                                                                                                                                                               
		<headerContent>                                                                                                                                                                                                                                              
			<m:FlexBox wrap="Wrap" fitContainer="true">                                                                                                                                                                                                                 
				<m:Avatar class="sapUiMediumMarginEnd sapUiSmallMarginBottom" displayShape="Circle" badgeIcon="sap-icon://internet-browser" displaySize="L"/>                                                                                                              
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                                     
                                                                                                                                                                                                                                                               
				<m:VBox class="sapUiLargeMarginEnd sapUiSmallMarginBottom">                                                                                                                                                                                                
					<m:Title level="H4" titleStyle="H4" text="Plant" class="sapUiTinyMarginBottom">                                                                                                                                                                           
					</m:Title>                                                                                                                                                                                                                                                
					<m:Label text="{plantsModel>/plant}"/>                                                                                                                                                                                                                    
				</m:VBox>                                                                                                                                                                                                                                                  
                                                                                                                                                                                                                                                               
				<m:VBox class="sapUiSmallMarginBottom sapUiLargeMarginEnd">                                                                                                                                                                                                
					<m:Title level="H4" titleStyle="H4" text="Property" class="sapUiTinyMarginBottom" />                                                                                                                                                                      
					<m:Label text="{plantsModel>/property}"/>                                                                                                                                                                                                                 
				</m:VBox>                                                                                                                                                                                                                                                  
                                                                                                                                                                                                                                                               
				<m:VBox class="sapUiSmallMarginBottom">                                                                                                                                                                                                                    
					<m:Title level="H4" titleStyle="H4" text="Address" class="sapUiTinyMarginBottom" />                                                                                                                                                                       
					<m:Label wrapping="true" text="{plantsModel>/address}"/>                                                                                                                                                                                                  
				</m:VBox>                                                                                                                                                                                                                                                  
			</m:FlexBox>                                                                                                                                                                                                                                                
		</headerContent>                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                               
		<sections>                                                                                                                                                                                                                                                   
                                                                                                                                                                                                                                                               
			<ObjectPageSection titleUppercase="false" id="personalSection" title="Web Details">                                                                                                                                                                         
				<subSections>                                                                                                                                                                                                                                              
					<ObjectPageSubSection id="personalSectionSS1"  titleUppercase="false">                                                                                                                                                                                    
						<blocks>                                                                                                                                                                                                                                                 
                            <f:SimpleForm id="basicDetails" editable="true" layout="ResponsiveGridLayout" labelSpanXL="4" labelSpanL="4" labelSpanM="12" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0" emptySpanL="0" emptySpanM="0" emptySpanS="0" +
columnsXL="2" columnsL="2" columnsM="2" singleContainerFullSize="false">                                                                                                                                                                                       
                                <f:content>                                                                                                                                                                                                                    
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                   
                                    <core:Title text="" visible="false"/>                                                                                                                                                                                      
                                    <m:Label  text="{i18n>propLat}" />                                                                                                                                                                                         
                                    <m:Input enabled="false" id="propLat"  valueStateText="Value cannot be blank" valueState="{/PropertyLatitude}" value="{plantBasicDetailsModel>/PropertyLatitude}" />                                                       
                                    <m:Label  text="{i18n>propLong}" />                                                                                                                                                                                        
                                    <m:Input enabled="false" id="propLong"  valueStateText="Value cannot be blank" valueState="{/PropertyLongitude}" value="{plantBasicDetailsModel>/PropertyLongitude}" />                                                    
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                               
                                    <m:Label  text="{i18n>propAdminFee}" />                                                                                                                                                                                    
                                    <m:Input enabled="false" id="propAdmin"  valueStateText="Value cannot be blank" valueState="{/PropertyAdminFee}" type="Number"&nbsp;                                                                                       
                                    value="{parts: ['plantBasicDetailsModel>/PropertyAdminFee'], formatter: '.formatter.removeLeadingZeros'}"/>                                                                                                                
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                               
                                    <m:Label  text="{i18n>adminFeeEff}" />                                                                                                                                                                                     
                                    <m:DatePicker enabled="false" id="effDate" valueStateText="Value cannot be blank" valueState="{/AdminFeeEffectiveDate}" placeholder="Choose Date..." value="{&nbsp;                                                        
                                        parts: ['plantBasicDetailsModel>/AdminFeeEffectiveDate'], formatter: '.formatter.formatDateTime',                                                                                                                      
                                        type: 'sap.ui.model.type.Date'&nbsp;&nbsp;                                                                                                                                                                             
                                      }" valueFormat="yyyy-MM-dd" />                                                                                                                                                                                           
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                               
                                    <m:Label text="{i18n>propChurn}" />                                                                                                                                                                                        
                                    <m:Input id="propChurn" enabled="false" showValueHelp="false" valueHelpOnly="true" valueHelpRequest="_onValueHelpChurnStatus" value="{plantBasicDetailsModel>/PropertyChurnStatus}" />                                     
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                               
                                    <m:Label text="{i18n>climateControl}" />                                                                                                                                                                                   
                                        <m:Input id="climControl" enabled="false" showValueHelp="false" valueHelpOnly="true" valueHelpRequest="_onValueHelpClimateControl" value="{plantBasicDetailsModel>/ClimateControl}" />                                 
                                    <!-- <m:Select id="climControl" selectedKey="{parts: ['plantBasicDetailsModel>/ClimateControl'], formatter: '.formatter.getSelectedKeyBlank'}">                                                                            
                                        <m:items>                                                                                                                                                                                                              
                                            <core:Item text="Yes" key="Y"/>                                                                                                                                                                                    
                                            <core:Item text="No" key="N"/>                                                                                                                                                                                     
                                            <core:Item text="" key="B"/>                                                                                                                                                                                       
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                               
                                        </m:items>                                                                                                                                                                                                             
                                    </m:Select> -->                                                                                                                                                                                                            
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                               
                                    <m:Label  text="{i18n>propWebReservation}" />                                                                                                                                                                              
                                    <m:Select enabled="false" id="propWeb" valueStateText="Value cannot be blank" selectedKey="{parts: ['plantBasicDetailsModel>/PropertyWebsiteReservations'], formatter: '.formatter.getSelectedKeyBlank'}"                  
                                    valueState="{/PropertyWebsiteReservations}">                                                                                                                                                                               
                                        <m:items>                                                                                                                                                                                                              
                                            <core:Item text="Yes" key="Y"/>                                                                                                                                                                                    
                                            <core:Item text="No" key="N"/>                                                                                                                                                                                     
                                            <core:Item text="" key="B"/>                                                                                                                                                                                       
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                               
                                        </m:items>                                                                                                                                                                                                             
                                    </m:Select>                                                                                                                                                                                                                
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                               
                                    <m:Label text="{i18n>webEnabledDate}" />                                                                                                                                                                                   
                                    <m:DatePicker enabled="false" id="webenabled" valueStateText="Value cannot be blank" valueState="{/WebsiteEnabledDate}" placeholder="Choose Date..."                                                                       
                                    value="{&nbsp;                                                                                                                                                                                                             
                                        parts: ['plantBasicDetailsModel>/WebsiteEnabledDate'], formatter: '.formatter.formatDateTime',                                                                                                                         
                                        type: 'sap.ui.model.type.Date'&nbsp;&nbsp;                                                                                                                                                                             
                                      }" valueFormat="yyyy-MM-dd"/>                                                                                                                                                                                            
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                               
                                    <m:Label  text="{i18n>propCallCenter}" />                                                                                                                                                                                  
                                    <m:Select enabled="false" id="propCall" valueStateText="Value cannot be blank" selectedKey="{parts: ['plantBasicDetailsModel>/PropertyCallCenterReservati'], formatter: '.formatter.getSelectedKeyBlank'}"                 
                                    valueState="{/PropertyCallCenterReservati}">                                                                                                                                                                               
                                        <m:items>                                                                                                                                                                                                              
                                            <core:Item text="Yes" key="Y"/>                                                                                                                                                                                    
                                            <core:Item text="No" key="N"/>                                                                                                                                                                                     
                                            <core:Item text="" key="B"/>                                                                                                                                                                                       
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                               
                                        </m:items>                                                                                                                                                                                                             
                                    </m:Select>                                                                                                                                                                                                                
                                    <core:Title text="" />                                                                                                                                                                                                     
                                    <m:Label text="{i18n>callCenterEnabled}" />                                                                                                                                                                                
                                    <m:DatePicker enabled="false" id="callCenter" valueStateText="Value cannot be blank" valueState="{/CallCenterEnabledDate}" placeholder="Choose Date..."&nbsp;                                                              
                                    value="{&nbsp;                                                                                                                                                                                                             
                                        parts: ['plantBasicDetailsModel>/CallCenterEnabledDate'], formatter: '.formatter.formatDateTime',                                                                                                                      
                                        type: 'sap.ui.model.type.Date'&nbsp;&nbsp;                                                                                                                                                                             
                                      }" valueFormat="yyyy-MM-dd"/>                                                                                                                                                                                            
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                               
                                    <m:Label  text="{i18n>propNFSFee}" />                                                                                                                                                                                      
                                    <m:Input enabled="false" id="propNFS" type="Number" valueStateText="Value cannot be blank" valueState="{/PropertyNfsFee}"                                                                                                  
                                     value="{parts: ['plantBasicDetailsModel>/PropertyNfsFee'], formatter: '.formatter.removeLeadingZeros'}"/>                                                                                                                 
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                               
                                    <m:Label  text="{i18n>maxResDays}" />                                                                                                                                                                                      
                                    <m:Input enabled="false" id="maxRes" valueStateText="Value cannot be blank" type="Number" maxLength="3" valueState="{/MaxReservationDays}"&nbsp;                                                                           
                                    value="{parts: ['plantBasicDetailsModel>/MaxReservationDays'], formatter: '.formatter.removeLeadingZeros'}" />                                                                                                             
                                    <m:Label text="{i18n>nsfFeeEff}" />                                                                                                                                                                                        
                                    <m:DatePicker enabled="false" id="nsfFeeEff" placeholder="Choose Date..."&nbsp;                                                                                                                                            
                                    value="{&nbsp;                                                                                                                                                                                                             
                                        parts: ['plantBasicDetailsModel>/NsfFeeEffectiveDate'], formatter: '.formatter.formatDateTime',                                                                                                                        
                                        type: 'sap.ui.model.type.Date'&nbsp;&nbsp;                                                                                                                                                                             
                                      }" valueFormat="yyyy-MM-dd"/>                                                                                                                                                                                            
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                               
                                    <m:Label  text="{i18n>propNFS}" />                                                                                                                                                                                         
                                    <m:Input id="propNFSACH" enabled="false" valueStateText="Value cannot be blank" type="Number" valueState="{/PropertyNfsAchFee}"&nbsp;                                                                                      
                                    value="{parts: ['plantBasicDetailsModel>/PropertyNfsAchFee'], formatter: '.formatter.removeLeadingZeros'}"/>                                                                                                               
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                               
                                    <m:Label text="{i18n>nfsachfeeEffective}" />                                                                                                                                                                               
                                    <m:DatePicker enabled="false" id="nfsAshfeeeff" placeholder="Choose Date..."&nbsp;                                                                                                                                         
                                    value="{&nbsp;                                                                                                                                                                                                             
                                        parts: ['plantBasicDetailsModel>/NfsAchFeeEffectiveDate'], formatter: '.formatter.formatDateTime',                                                                                                                     
                                        type: 'sap.ui.model.type.Date'&nbsp;&nbsp;                                                                                                                                                                             
                                      }" valueFormat="yyyy-MM-dd"/>                                                                                                                                                                                            
                                    <m:Label  text="{i18n>propInsuranceFrozen}" />                                                                                                                                                                             
                                    <m:Select enabled="false" id="propFrozen" valueStateText="Value cannot be blank" valueState="{/PropertyInsuranceFrozen}" selectedKey="{plantBasicDetailsModel>/PropertyInsuranceFrozen}">                                  
                                        <m:items>                                                                                                                                                                                                              
                                            <core:Item text="Yes" key="Y"/>                                                                                                                                                                                    
                                            <core:Item text="No" key="N"/>                                                                                                                                                                                     
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                               
                                        </m:items>                                                                                                                                                                                                             
                                    </m:Select>                                                                                                                                                                                                                
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                               
                                    <m:Label  text="{i18n>propInsCancel}" />                                                                                                                                                                                   
                                    <m:Input id="propIns" enabled="false"  maxLength="3" valueStateText="Value cannot be blank" valueState="{/PropertyInsuranceCancelDay}"&nbsp;                                                                               
                                    value="{plantBasicDetailsModel>/PropertyInsuranceCancelDay}"/>                                                                                                                                                             
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                               
                                    <m:Label  text="{i18n>preResDay}" />                                                                                                                                                                                       
                                    <m:Input id="propResDay" enabled="false"  maxLength="3" valueStateText="Value cannot be blank" valueState="{/PreReservationDays}"                                                                                          
                                     value="{parts: ['plantBasicDetailsModel>/PreReservationDays'], formatter: '.formatter.removeLeadingZeros'}" />                                                                                                            
                                </f:content>                                                                                                                                                                                                                   
                            </f:SimpleForm>                                                                                                                                                                                                                    
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                       
						</blocks>                                                                                                                                                                                                                                                
					</ObjectPageSubSection>                                                                                                                                                                                                                                   
				</subSections>                                                                                                                                                                                                                                             
			</ObjectPageSection>                                                                                                                                                                                                                                        
                                                                                                                                                                                                                                                               
		</sections>                                                                                                                                                                                                                                                  
	</ObjectPageLayout>                                                                                                                                                                                                                                           
</mvc:View>                                                                                                                                                                                                                                                    