<mvc:View xmlns:mvc="sap.ui.core.mvc"                                                                                                                                                                                                                          
controllerName="com.public.storage.paoro.controller.InsuranceDetails"                                                                                                                                                                                          
	xmlns="sap.uxap"                                                                                                                                                                                                                                              
	xmlns:m="sap.m"                                                                                                                                                                                                                                               
	xmlns:core="sap.ui.core"                                                                                                                                                                                                                                      
	xmlns:l="sap.ui.layout"                                                                                                                                                                                                                                       
	xmlns:f="sap.ui.layout.form" height="100%">                                                                                                                                                                                                                   
                                                                                                                                                                                                                                                               
	<ObjectPageLayout id="ObjectPageLayout" upperCaseAnchorBar="false">                                                                                                                                                                                           
		<headerTitle>                                                                                                                                                                                                                                                
			<ObjectPageDynamicHeaderTitle>                                                                                                                                                                                                                              
				<expandedHeading>                                                                                                                                                                                                                                          
					<m:Title text="Insurance Details" wrapping="true" />                                                                                                                                                                                                      
				</expandedHeading>                                                                                                                                                                                                                                         
                                                                                                                                                                                                                                                               
				<snappedHeading>                                                                                                                                                                                                                                           
					<m:HBox>                                                                                                                                                                                                                                                  
						<m:VBox>                                                                                                                                                                                                                                                 
							<m:Title text="Insurance Details" wrapping="true"/>                                                                                                                                                                                                     
							<m:Label text="Fill your insurance details"/>                                                                                                                                                                                                           
						</m:VBox>                                                                                                                                                                                                                                                
					</m:HBox>                                                                                                                                                                                                                                                 
				</snappedHeading>                                                                                                                                                                                                                                          
                                                                                                                                                                                                                                                               
				<expandedContent>                                                                                                                                                                                                                                          
					<m:Label text="Fill your insurance details"/>                                                                                                                                                                                                             
				</expandedContent>                                                                                                                                                                                                                                         
                                                                                                                                                                                                                                                               
				<snappedTitleOnMobile>                                                                                                                                                                                                                                     
					<m:Title text="Insurance Details"/>                                                                                                                                                                                                                       
				</snappedTitleOnMobile>                                                                                                                                                                                                                                    
                                                                                                                                                                                                                                                               
			</ObjectPageDynamicHeaderTitle>                                                                                                                                                                                                                             
		</headerTitle>                                                                                                                                                                                                                                               
                                                                                                                                                                                                                                                               
		<headerContent>                                                                                                                                                                                                                                              
			<m:FlexBox wrap="Wrap" fitContainer="true">                                                                                                                                                                                                                 
				<m:Avatar class="sapUiMediumMarginEnd sapUiSmallMarginBottom" src="sap-icon://insurance-house" displayShape="Circle" initials="ID" displaySize="L"/>                                                                                                       
                                                                                                                                                                                                                                                               
				<m:VBox class="sapUiLargeMarginEnd sapUiSmallMarginBottom">                                                                                                                                                                                                
					<m:Title level="H4" titleStyle="H4" text="Plant" class="sapUiTinyMarginBottom">                                                                                                                                                                           
					</m:Title>                                                                                                                                                                                                                                                
					<m:Label text="{plantsModel>/plant}"/>                                                                                                                                                                                                                    
				</m:VBox>                                                                                                                                                                                                                                                  
                                                                                                                                                                                                                                                               
				<m:VBox class="sapUiSmallMarginBottom sapUiLargeMarginEnd">                                                                                                                                                                                                
					<m:Title level="H4" titleStyle="H4" text="Property" class="sapUiTinyMarginBottom" />                                                                                                                                                                      
					<m:Label text="{plantsModel>/property}"/>                                                                                                                                                                                                                 
				</m:VBox>                                                                                                                                                                                                                                                  
                                                                                                                                                                                                                                                               
				<m:VBox class="sapUiSmallMarginBottom">                                                                                                                                                                                                                    
					<m:Title level="H4" titleStyle="H4" text="Address" class="sapUiTinyMarginBottom" />                                                                                                                                                                       
					<m:Label wrapping="true" text="{plantsModel>/address}"/>                                                                                                                                                                                                  
				</m:VBox>                                                                                                                                                                                                                                                  
			</m:FlexBox>                                                                                                                                                                                                                                                
		</headerContent>                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                               
		<sections>                                                                                                                                                                                                                                                   
                                                                                                                                                                                                                                                               
			<ObjectPageSection titleUppercase="false" id="personalSection" title="Insurance Details">                                                                                                                                                                   
				<subSections>                                                                                                                                                                                                                                              
					<ObjectPageSubSection id="personalSectionSS1"  titleUppercase="false">                                                                                                                                                                                    
						<blocks>                                                                                                                                                                                                                                                 
              <f:SimpleForm id="basicDetails" editable="true" layout="ResponsiveGridLayout" labelSpanXL="4" labelSpanL="4" labelSpanM="12" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0" emptySpanL="0" emptySpanM="0" emptySpanS="0" columnsXL="2" +
columnsL="2" columnsM="2" singleContainerFullSize="false">                                                                                                                                                                                                     
                <f:content>                                                                                                                                                                                                                                    
                    <core:Title text="" visible="false"/>                                                                                                                                                                                                      
                    <m:Label text="{i18n>finrepnum1}" />                                                                                                                                                                                                       
                    <m:Input enabled="false" id="finrep1"  value="{plantBasicDetailsModel>/FinrepNum1}" />                                                                                                                                                     
                    <m:Label text="{i18n>finrepnum2}" />                                                                                                                                                                                                       
                    <m:Input enabled="false" id="finrep2" value="{plantBasicDetailsModel>/FinrepNum2}" />                                                                                                                                                      
                                                                                                                                                                                                                                                               
                    <m:Label text="{i18n>insPrem2}" />                                                                                                                                                                                                         
                    <m:Input enabled="false" id="insprem2"  valueState="{/InsPrem2000}" value="{plantBasicDetailsModel>/InsPrem2000}" />                                                                                                                       
                                                                                                                                                                                                                                                               
                    <m:Label text="{i18n>insPrem3}" />                                                                                                                                                                                                         
                    <m:Input enabled="false"  id="insprem3"  valueState="{/InsPrem3000}" value="{plantBasicDetailsModel>/InsPrem3000}" />                                                                                                                      
                                                                                                                                                                                                                                                               
                    <m:Label text="{i18n>insPrem4}" />                                                                                                                                                                                                         
                    <m:Input enabled="false"  id="insprem4"  valueState="{/InsPrem4000}" value="{plantBasicDetailsModel>/InsPrem4000}" />                                                                                                                      
                                                                                                                                                                                                                                                               
                    <m:Label text="{i18n>insPrem5}" />                                                                                                                                                                                                         
                    <m:Input enabled="false"  id="insprem5"  valueState="{/InsPrem5000}" value="{plantBasicDetailsModel>/InsPrem5000}" />                                                                                                                      
                                                                                                                                                                                                                                                               
                    <m:Label text="{i18n>finrepnum7}" />                                                                                                                                                                                                       
                    <m:Input enabled="false" id="finrep7"  value="{plantBasicDetailsModel>/FinrepNum7}" />                                                                                                                                                     
                                                                                                                                                                                                                                                               
                    <m:Label text="{i18n>finrepnum8}" />                                                                                                                                                                                                       
                    <m:Input enabled="false" id="finrep8"  value="{plantBasicDetailsModel>/FinrepNum8}" />                                                                                                                                                     
                                                                                                                                                                                                                                                               
                    <m:Label text="{i18n>finrepnum9}" />                                                                                                                                                                                                       
                    <m:Input enabled="false" id="finrep9"  value="{plantBasicDetailsModel>/FinrepNum9}" />                                                                                                                                                     
                    <m:Label  text="{i18n>eRentalMax}" />                                                                                                                                                                                                      
                    <m:Input enabled="false" id="eRental"  valueState="{/ErentalMaxDays}" value="{plantBasicDetailsModel>/ErentalMaxDays}" />                                                                                                                  
                                                                                                                                                                                                                                                               
                    <core:Title text="" />                                                                                                                                                                                                                     
                                                                                                                                                                                                                                                               
                    <m:Label text="{i18n>cddStartDateN}" />                                                                                                                                                                                                    
                    <m:DatePicker enabled="false" id="startdate" placeholder="Choose Date.."  valueState="{/CddStartDate}" value="{&nbsp;                                                                                                                      
                        parts: ['plantBasicDetailsModel>/FinrepDate2'], formatter: '.formatter.formatDateTime',                                                                                                                                                
                        type: 'sap.ui.model.type.Date'&nbsp;&nbsp;                                                                                                                                                                                             
                      }" valueFormat="yyyy-MM-dd" displayFormat="yyyy-MM-dd" />                                                                                                                                                                                
                                                                                                                                                                                                                                                               
                    <m:Label text="{i18n>finrepD3}" />                                                                                                                                                                                                         
                    <m:DatePicker enabled="false" id="finrepD3" placeholder="Choose Date.." value="{&nbsp;                                                                                                                                                     
                        parts: ['plantBasicDetailsModel>/FinrepDate3'], formatter: '.formatter.formatDateTime',                                                                                                                                                
                        type: 'sap.ui.model.type.Date'&nbsp;&nbsp;                                                                                                                                                                                             
                      }" valueFormat="yyyy-MM-dd"/>                                                                                                                                                                                                            
                                                                                                                                                                                                                                                               
                    <m:Label text="{i18n>finrepD4}" />                                                                                                                                                                                                         
                    <m:DatePicker enabled="false" id="finrepD4" placeholder="Choose Date.." value="{&nbsp;                                                                                                                                                     
                        parts: ['plantBasicDetailsModel>/FinrepDate4'], formatter: '.formatter.formatDateTime',                                                                                                                                                
                        type: 'sap.ui.model.type.Date'&nbsp;&nbsp;                                                                                                                                                                                             
                      }" valueFormat="yyyy-MM-dd" />                                                                                                                                                                                                           
                    <m:Label text="{i18n>finrepD5}" />                                                                                                                                                                                                         
                    <m:DatePicker enabled="false" id="finrepD5" placeholder="Choose Date.." value="{&nbsp;                                                                                                                                                     
                        parts: ['plantBasicDetailsModel>/FinrepDate5'], formatter: '.formatter.formatDateTime',                                                                                                                                                
                        type: 'sap.ui.model.type.Date'&nbsp;&nbsp;                                                                                                                                                                                             
                      }" valueFormat="yyyy-MM-dd"/>                                                                                                                                                                                                            
                                                                                                                                                                                                                                                               
                    <m:Label text="{i18n>finrepD6}" />                                                                                                                                                                                                         
                    <m:DatePicker enabled="false" id="finrepD6" placeholder="Choose Date.." value="{&nbsp;                                                                                                                                                     
                        parts: ['plantBasicDetailsModel>/FinrepDate6'], formatter: '.formatter.formatDateTime',                                                                                                                                                
                        type: 'sap.ui.model.type.Date'                                                                                                                                                                                                         
                      }" valueFormat="yyyy-MM-dd" />                                                                                                                                                                                                           
                                                                                                                                                                                                                                                               
                    <m:Label text="{i18n>finrepD7}" />                                                                                                                                                                                                         
                    <m:DatePicker enabled="false" id="finrepD7" placeholder="Choose Date.." value="{&nbsp;                                                                                                                                                     
                        parts: ['plantBasicDetailsModel>/FinrepDate7'], formatter: '.formatter.formatDateTime',                                                                                                                                                
                        type: 'sap.ui.model.type.Date'&nbsp;&nbsp;                                                                                                                                                                                             
                      }" valueFormat="yyyy-MM-dd" />                                                                                                                                                                                                           
                    <m:Label text="{i18n>finrepD8}" />                                                                                                                                                                                                         
                    <m:DatePicker enabled="false" id="finrepD8" value="{&nbsp;                                                                                                                                                                                 
                        parts: ['plantBasicDetailsModel>/FinrepDate8'], formatter: '.formatter.formatDateTime',                                                                                                                                                
                        type: 'sap.ui.model.type.Date'&nbsp;&nbsp;                                                                                                                                                                                             
                      }" valueFormat="yyyy-MM-dd" placeholder="Choose Date.."/>                                                                                                                                                                                
                    <m:Label text="{i18n>finrepD9}" />                                                                                                                                                                                                         
                    <m:DatePicker enabled="false" id="finrepD9" value="{&nbsp;                                                                                                                                                                                 
                        parts: ['plantBasicDetailsModel>/FinrepDate9'], formatter: '.formatter.formatDateTime',                                                                                                                                                
                        type: 'sap.ui.model.type.Date'&nbsp;&nbsp;                                                                                                                                                                                             
                      }" valueFormat="yyyy-MM-dd" placeholder="Choose Date.." />                                                                                                                                                                               
                    <m:Label text="{i18n>finrepD10}" />                                                                                                                                                                                                        
                    <m:DatePicker enabled="false" id="finrepD10" value="{&nbsp;                                                                                                                                                                                
                        parts: ['plantBasicDetailsModel>/FinrepDate10'], formatter: '.formatter.formatDateTime',                                                                                                                                               
                        type: 'sap.ui.model.type.Date'&nbsp;&nbsp;                                                                                                                                                                                             
                      }" valueFormat="yyyy-MM-dd" placeholder="Choose Date.."/>                                                                                                                                                                                
                                                                                                                                                                                                                                                               
                </f:content>                                                                                                                                                                                                                                   
            </f:SimpleForm>                                                                                                                                                                                                                                    
						</blocks>                                                                                                                                                                                                                                                
					</ObjectPageSubSection>                                                                                                                                                                                                                                   
				</subSections>                                                                                                                                                                                                                                             
			</ObjectPageSection>                                                                                                                                                                                                                                        
                                                                                                                                                                                                                                                               
		</sections>                                                                                                                                                                                                                                                  
	</ObjectPageLayout>                                                                                                                                                                                                                                           
</mvc:View>                                                                                                                                                                                                                                                    