<mvc:View xmlns:mvc="sap.ui.core.mvc"                                                                                                                                                                                                                          
controllerName="com.public.storage.paoro.controller.PropDates"                                                                                                                                                                                                 
	xmlns="sap.uxap"                                                                                                                                                                                                                                              
	xmlns:m="sap.m"                                                                                                                                                                                                                                               
	xmlns:core="sap.ui.core"                                                                                                                                                                                                                                      
	xmlns:l="sap.ui.layout"                                                                                                                                                                                                                                       
	xmlns:f="sap.ui.layout.form" height="100%">                                                                                                                                                                                                                   
                                                                                                                                                                                                                                                               
	<ObjectPageLayout id="ObjectPageLayout" upperCaseAnchorBar="false">                                                                                                                                                                                           
		<headerTitle>                                                                                                                                                                                                                                                
			<ObjectPageDynamicHeaderTitle>                                                                                                                                                                                                                              
				<expandedHeading>                                                                                                                                                                                                                                          
					<m:Title text="Property Date Details" wrapping="true" />                                                                                                                                                                                                  
				</expandedHeading>                                                                                                                                                                                                                                         
                                                                                                                                                                                                                                                               
				<snappedHeading>                                                                                                                                                                                                                                           
					<m:HBox>                                                                                                                                                                                                                                                  
						<m:VBox>                                                                                                                                                                                                                                                 
							<m:Title text="Property Date Details" wrapping="true"/>                                                                                                                                                                                                 
							<m:Label text="Fill your property date details"/>                                                                                                                                                                                                       
						</m:VBox>                                                                                                                                                                                                                                                
					</m:HBox>                                                                                                                                                                                                                                                 
				</snappedHeading>                                                                                                                                                                                                                                          
                                                                                                                                                                                                                                                               
				<expandedContent>                                                                                                                                                                                                                                          
					<m:Label text="Fill your property date details"/>                                                                                                                                                                                                         
				</expandedContent>                                                                                                                                                                                                                                         
                                                                                                                                                                                                                                                               
				<snappedTitleOnMobile>                                                                                                                                                                                                                                     
					<m:Title text="Property Date Details"/>                                                                                                                                                                                                                   
				</snappedTitleOnMobile>                                                                                                                                                                                                                                    
                                                                                                                                                                                                                                                               
			                                                                                                                                                                                                                                                            
			</ObjectPageDynamicHeaderTitle>                                                                                                                                                                                                                             
		</headerTitle>                                                                                                                                                                                                                                               
                                                                                                                                                                                                                                                               
		<headerContent>                                                                                                                                                                                                                                              
			<m:FlexBox wrap="Wrap" fitContainer="true">                                                                                                                                                                                                                 
				<m:Avatar class="sapUiMediumMarginEnd sapUiSmallMarginBottom" src="sap-icon://appointment-2" displayShape="Circle" initials="PD" displaySize="L"/>                                                                                                         
                                                                                                                                                                                                                                                               
				<m:VBox class="sapUiLargeMarginEnd sapUiSmallMarginBottom">                                                                                                                                                                                                
					<m:Title level="H4" titleStyle="H4" text="Plant" class="sapUiTinyMarginBottom">                                                                                                                                                                           
					</m:Title>                                                                                                                                                                                                                                                
					<m:Label text="{plantsModel>/plant}"/>                                                                                                                                                                                                                    
				</m:VBox>                                                                                                                                                                                                                                                  
                                                                                                                                                                                                                                                               
				<m:VBox class="sapUiSmallMarginBottom sapUiLargeMarginEnd">                                                                                                                                                                                                
					<m:Title level="H4" titleStyle="H4" text="Property" class="sapUiTinyMarginBottom" />                                                                                                                                                                      
					<m:Label text="{plantsModel>/property}"/>                                                                                                                                                                                                                 
				</m:VBox>                                                                                                                                                                                                                                                  
                                                                                                                                                                                                                                                               
				<m:VBox class="sapUiSmallMarginBottom">                                                                                                                                                                                                                    
					<m:Title level="H4" titleStyle="H4" text="Address" class="sapUiTinyMarginBottom" />                                                                                                                                                                       
					<m:Label wrapping="true" text="{plantsModel>/address}"/>                                                                                                                                                                                                  
				</m:VBox>                                                                                                                                                                                                                                                  
			</m:FlexBox>                                                                                                                                                                                                                                                
		</headerContent>                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                               
		<sections>                                                                                                                                                                                                                                                   
                                                                                                                                                                                                                                                               
			<ObjectPageSection titleUppercase="false" id="personalSection" title="Property Date Details">                                                                                                                                                               
				<subSections>                                                                                                                                                                                                                                              
					<ObjectPageSubSection id="personalSectionSS1"  titleUppercase="false">                                                                                                                                                                                    
						<blocks>                                                                                                                                                                                                                                                 
              <f:SimpleForm id="basicDetails" editable="true" layout="ResponsiveGridLayout" labelSpanXL="4" labelSpanL="4" labelSpanM="12" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0" emptySpanL="0" emptySpanM="0" emptySpanS="0" columnsXL="2" +
columnsL="2" columnsM="2" singleContainerFullSize="false">                                                                                                                                                                                                     
                <f:content>                                                                                                                                                                                                                                    
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                         
                    <m:Label  text="{i18n>restrtdate}" />                                                                                                                                                                                                      
                    <m:DatePicker enabled="false" id="restrtdate" value="{&nbsp;                                                                                                                                                                               
                        parts: ['plantBasicDetailsModel>/ReStartDate'], formatter: '.formatter.formatDateTime',                                                                                                                                                
                        type: 'sap.ui.model.type.Date'                                                                                                                                                                                                         
                      }" valueFormat="yyyy-MM-dd"  placeholder="Choose Date.."/>                                                                                                                                                                               
                                                                                                                                                                                                                                                               
                    <m:Label  text="{i18n>newOwnerdate}" />                                                                                                                                                                                                    
                    <m:DatePicker enabled="false" id="newOwnerdate" value="{&nbsp;                                                                                                                                                                             
                        parts: ['plantBasicDetailsModel>/NewOwnershipDate'], formatter: '.formatter.formatDateTime',                                                                                                                                           
                        type: 'sap.ui.model.type.Date'&nbsp;&nbsp;                                                                                                                                                                                             
                      }" valueFormat="yyyy-MM-dd"  placeholder="Choose Date.." />                                                                                                                                                                              
                                                                                                                                                                                                                                                               
                    <m:Label text="{i18n>orstartdate}" />                                                                                                                                                                                                      
                    <m:DatePicker enabled="false" id="orstartdate" value="{&nbsp;                                                                                                                                                                              
                        parts: ['plantBasicDetailsModel>/OriginalStartDate'], formatter: '.formatter.formatDateTime',                                                                                                                                          
                        type: 'sap.ui.model.type.Date'&nbsp;&nbsp;                                                                                                                                                                                             
                      }" valueFormat="yyyy-MM-dd" valueStateText="Value cannot be blank" valueState="{/OriginalStartDate}" placeholder="Choose Date.." />                                                                                                      
                                                                                                                                                                                                                                                               
                    <m:Label  text="{i18n>terminationdate}" />                                                                                                                                                                                                 
                    <m:DatePicker enabled="false" id="terminationdate" value="{&nbsp;                                                                                                                                                                          
                        parts: ['plantBasicDetailsModel>/TerminationDate'], formatter: '.formatter.formatDateTime',                                                                                                                                            
                        type: 'sap.ui.model.type.Date'&nbsp;&nbsp;                                                                                                                                                                                             
                      }" valueFormat="yyyy-MM-dd"  placeholder="Choose Date.." />                                                                                                                                                                              
                                                                                                                                                                                                                                                               
                    <m:Label text="{i18n>buCreate}" />                                                                                                                                                                                                         
                    <m:DatePicker enabled="false" id="bucreatedate" value="{&nbsp;                                                                                                                                                                             
                        parts: ['plantBasicDetailsModel>/BuCreateDate'], formatter: '.formatter.formatDateTime',                                                                                                                                               
                        type: 'sap.ui.model.type.Date'&nbsp;&nbsp;                                                                                                                                                                                             
                      }" valueFormat="yyyy-MM-dd"  placeholder="Choose Date.." />                                                                                                                                                                              
                </f:content>                                                                                                                                                                                                                                   
            </f:SimpleForm>                                                                                                                                                                                                                                    
						</blocks>                                                                                                                                                                                                                                                
					</ObjectPageSubSection>                                                                                                                                                                                                                                   
				</subSections>                                                                                                                                                                                                                                             
			</ObjectPageSection>                                                                                                                                                                                                                                        
                                                                                                                                                                                                                                                               
		</sections>                                                                                                                                                                                                                                                  
	</ObjectPageLayout>                                                                                                                                                                                                                                           
</mvc:View>                                                                                                                                                                                                                                                    