<mvc:View xmlns:mvc="sap.ui.core.mvc"                                                                                                                                                                                                                          
controllerName="com.public.storage.paoro.controller.MarketingDetails"                                                                                                                                                                                          
	xmlns="sap.uxap"                                                                                                                                                                                                                                              
	xmlns:m="sap.m"                                                                                                                                                                                                                                               
	xmlns:core="sap.ui.core"                                                                                                                                                                                                                                      
	xmlns:l="sap.ui.layout"                                                                                                                                                                                                                                       
	xmlns:f="sap.ui.layout.form" height="100%">                                                                                                                                                                                                                   
                                                                                                                                                                                                                                                               
	<ObjectPageLayout id="ObjectPageLayout" upperCaseAnchorBar="false">                                                                                                                                                                                           
		<headerTitle>                                                                                                                                                                                                                                                
			<ObjectPageDynamicHeaderTitle>                                                                                                                                                                                                                              
				<expandedHeading>                                                                                                                                                                                                                                          
					<m:Title text="Market Details" wrapping="true" />                                                                                                                                                                                                         
				</expandedHeading>                                                                                                                                                                                                                                         
                                                                                                                                                                                                                                                               
				<snappedHeading>                                                                                                                                                                                                                                           
					<m:HBox>                                                                                                                                                                                                                                                  
						<m:VBox>                                                                                                                                                                                                                                                 
							<m:Title text="Market Details" wrapping="true"/>                                                                                                                                                                                                        
							<m:Label text="Fill your market details"/>                                                                                                                                                                                                              
						</m:VBox>                                                                                                                                                                                                                                                
					</m:HBox>                                                                                                                                                                                                                                                 
				</snappedHeading>                                                                                                                                                                                                                                          
                                                                                                                                                                                                                                                               
				<expandedContent>                                                                                                                                                                                                                                          
					<m:Label text="Fill your market details"/>                                                                                                                                                                                                                
				</expandedContent>                                                                                                                                                                                                                                         
                                                                                                                                                                                                                                                               
				<snappedTitleOnMobile>                                                                                                                                                                                                                                     
					<m:Title text="Market Details"/>                                                                                                                                                                                                                          
				</snappedTitleOnMobile>                                                                                                                                                                                                                                    
                                                                                                                                                                                                                                                               
				                                                                                                                                                                                                                                                           
			</ObjectPageDynamicHeaderTitle>                                                                                                                                                                                                                             
		</headerTitle>                                                                                                                                                                                                                                               
                                                                                                                                                                                                                                                               
		<headerContent>                                                                                                                                                                                                                                              
			<m:FlexBox wrap="Wrap" fitContainer="true">                                                                                                                                                                                                                 
				<m:Avatar class="sapUiMediumMarginEnd sapUiSmallMarginBottom" src="sap-icon://opportunity" displayShape="Circle" initials="MD" displaySize="L"/>                                                                                                           
                                                                                                                                                                                                                                                               
				<m:VBox class="sapUiLargeMarginEnd sapUiSmallMarginBottom">                                                                                                                                                                                                
					<m:Title level="H4" titleStyle="H4" text="Plant" class="sapUiTinyMarginBottom">                                                                                                                                                                           
					</m:Title>                                                                                                                                                                                                                                                
					<m:Label text="{plantsModel>/plant}"/>                                                                                                                                                                                                                    
				</m:VBox>                                                                                                                                                                                                                                                  
                                                                                                                                                                                                                                                               
				<m:VBox class="sapUiSmallMarginBottom sapUiLargeMarginEnd">                                                                                                                                                                                                
					<m:Title level="H4" titleStyle="H4" text="Property" class="sapUiTinyMarginBottom" />                                                                                                                                                                      
					<m:Label text="{plantsModel>/property}"/>                                                                                                                                                                                                                 
				</m:VBox>                                                                                                                                                                                                                                                  
                                                                                                                                                                                                                                                               
				<m:VBox class="sapUiSmallMarginBottom">                                                                                                                                                                                                                    
					<m:Title level="H4" titleStyle="H4" text="Address" class="sapUiTinyMarginBottom" />                                                                                                                                                                       
					<m:Label wrapping="true" text="{plantsModel>/address}"/>                                                                                                                                                                                                  
				</m:VBox>                                                                                                                                                                                                                                                  
			</m:FlexBox>                                                                                                                                                                                                                                                
		</headerContent>                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                               
		<sections>                                                                                                                                                                                                                                                   
                                                                                                                                                                                                                                                               
			<ObjectPageSection titleUppercase="false" id="personalSection" title="Marketing Details">                                                                                                                                                                   
				<subSections>                                                                                                                                                                                                                                              
					<ObjectPageSubSection id="personalSectionSS1"  titleUppercase="false">                                                                                                                                                                                    
						<blocks>                                                                                                                                                                                                                                                 
							<f:SimpleForm id="basicDetails" editable="true" layout="ResponsiveGridLayout" labelSpanXL="4" labelSpanL="4" labelSpanM="12" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0" emptySpanL="0" emptySpanM="0" emptySpanS="0" columnsXL="2" columns+
L="2" columnsM="2" singleContainerFullSize="false">                                                                                                                                                                                                            
								<f:content>                                                                                                                                                                                                                                            
									                                                                                                                                                                                                                                                      
									<m:Label  text="{i18n>marketKey}" />                                                                                                                                                                                                                  
									<m:Input id="markKey" enabled="false"  valueState="{/MarketKey}" showValueHelp="false" valueHelpRequest="_onValueHelpMarketKey" valueHelpOnly="true" value="{plantBasicDetailsModel>/MarketKey}" />                                                   
									<m:Label  text="{i18n>metroStatisticalArea}" />                                                                                                                                                                                                       
									<m:Input id="metroStats" enabled="false"  valueState="{/MetroStatisicalArea}" showValueHelp="false" valueHelpRequest="_onValueHelpMetroStats" valueHelpOnly="true" value="{plantBasicDetailsModel>/MetroStatisicalArea}" />                           
				                                                                                                                                                                                                                                                           
									<m:Label  text="{i18n>neighbourhood}" />                                                                                                                                                                                                              
									<m:Input id="neighbourwood" enabled="false"  valueState="{/Neighborwood}" showValueHelp="false" valueHelpRequest="_onValueHelpNeigbour" valueHelpOnly="true" value="{plantBasicDetailsModel>/Neighborwood}" />                                        
				                                                                                                                                                                                                                                                           
									<m:Label text="Same Store" />                                                                                                                                                                                                                         
									<m:Input id="psCons" enabled="false"  valueState="{/PsConsolidatedPropertygroup}" showValueHelp="false" valueHelpRequest="_onValueHelpConslidated" valueHelpOnly="true" value="{plantBasicDetailsModel>/SameStore}" />                                
				                                                                                                                                                                                                                                                           
									<m:Label text="{i18n>rankProp}" />                                                                                                                                                                                                                    
										<m:Select enabled="false" id="rankProp" selectedKey="{parts: ['plantBasicDetailsModel>/Rank'], formatter: '.formatter.getSelectedRankProp'}">                                                                                                        
											<m:items>																								                                                                                                                                                                                                                   
												<core:Item text="Yes" key="Y" />                                                                                                                                                                                                                   
												<core:Item text="No" key="N"/>                                                                                                                                                                                                                     
					                                                                                                                                                                                                                                                          
											</m:items>                                                                                                                                                                                                                                          
										</m:Select>                                                                                                                                                                                                                                          
                                                                                                                                                                                                                                                               
										<m:Label text="{i18n>commType}" />                                                                                                                                                                                                                   
										<m:Select enabled="false" id="commType" selectedKey="{parts: ['plantBasicDetailsModel>/CommunityType'], formatter: '.formatter.getCommType'}">                                                                                                       
											                                                                                                                                                                                                                                                    
											<m:items>                                                                                                                                                                                                                                           
												<core:Item text="" key="B" />																								                                                                                                                                                                                              
												<core:Item text="Not Assigned" key="NA" />                                                                                                                                                                                                         
												<core:Item text="Urban Centers" key="UC"/>                                                                                                                                                                                                         
												<core:Item text="Urban Suburbs" key="US" />                                                                                                                                                                                                        
												<core:Item text="Middle Suburbs" key="MS"/>                                                                                                                                                                                                        
												<core:Item text="Exurbs" key="EX" />                                                                                                                                                                                                               
					                                                                                                                                                                                                                                                          
											</m:items>                                                                                                                                                                                                                                          
										</m:Select>                                                                                                                                                                                                                                          
								</f:content>                                                                                                                                                                                                                                           
							</f:SimpleForm>                                                                                                                                                                                                                                         
						</blocks>                                                                                                                                                                                                                                                
					</ObjectPageSubSection>                                                                                                                                                                                                                                   
				</subSections>                                                                                                                                                                                                                                             
			</ObjectPageSection>                                                                                                                                                                                                                                        
                                                                                                                                                                                                                                                               
		</sections>                                                                                                                                                                                                                                                  
	</ObjectPageLayout>                                                                                                                                                                                                                                           
</mvc:View>                                                                                                                                                                                                                                                    