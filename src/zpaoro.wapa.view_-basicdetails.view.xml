<mvc:View xmlns:mvc="sap.ui.core.mvc" controllerName="com.public.storage.paoro.controller.BasicDetails"                                                                                                                                                        
	xmlns="sap.uxap"                                                                                                                                                                                                                                              
	xmlns:m="sap.m"                                                                                                                                                                                                                                               
	xmlns:core="sap.ui.core"                                                                                                                                                                                                                                      
	xmlns:l="sap.ui.layout"                                                                                                                                                                                                                                       
	xmlns:f="sap.ui.layout.form" height="100%">                                                                                                                                                                                                                   
                                                                                                                                                                                                                                                               
	<ObjectPageLayout id="ObjectPageLayout" upperCaseAnchorBar="false">                                                                                                                                                                                           
		<headerTitle>                                                                                                                                                                                                                                                
			<ObjectPageDynamicHeaderTitle>                                                                                                                                                                                                                              
                                                                                                                                                                                                                                                               
				<expandedHeading>                                                                                                                                                                                                                                          
					<m:Title text="Basic Details" wrapping="true" />                                                                                                                                                                                                          
                                                                                                                                                                                                                                                               
				</expandedHeading>                                                                                                                                                                                                                                         
                                                                                                                                                                                                                                                               
				<snappedHeading>                                                                                                                                                                                                                                           
                                                                                                                                                                                                                                                               
					<m:VBox>                                                                                                                                                                                                                                                  
						<m:Title text="Basic Details" wrapping="true"/>                                                                                                                                                                                                          
						<m:Label text="Fill your basic details"/>                                                                                                                                                                                                                
					</m:VBox>                                                                                                                                                                                                                                                 
                                                                                                                                                                                                                                                               
                                                                                                                                                                                                                                                               
                                                                                                                                                                                                                                                               
				</snappedHeading>                                                                                                                                                                                                                                          
                                                                                                                                                                                                                                                               
				<expandedContent>                                                                                                                                                                                                                                          
					<m:Label text="Fill your basic details"/>                                                                                                                                                                                                                 
				</expandedContent>                                                                                                                                                                                                                                         
                                                                                                                                                                                                                                                               
				<snappedTitleOnMobile>                                                                                                                                                                                                                                     
					<m:Title text="Basic Details"/>                                                                                                                                                                                                                           
				</snappedTitleOnMobile>                                                                                                                                                                                                                                    
                                                                                                                                                                                                                                                               
			</ObjectPageDynamicHeaderTitle>                                                                                                                                                                                                                             
		</headerTitle>                                                                                                                                                                                                                                               
                                                                                                                                                                                                                                                               
		<headerContent>                                                                                                                                                                                                                                              
			<m:FlexBox wrap="Wrap" fitContainer="true">                                                                                                                                                                                                                 
				<m:Avatar class="sapUiMediumMarginEnd sapUiSmallMarginBottom" displayShape="Circle" src="sap-icon://business-card" initials="BD" displaySize="L"/>                                                                                                         
                                                                                                                                                                                                                                                               
				<m:VBox class="sapUiLargeMarginEnd sapUiSmallMarginBottom">                                                                                                                                                                                                
					<m:Title level="H4" titleStyle="H4" text="Plant" class="sapUiTinyMarginBottom">                                                                                                                                                                           
					</m:Title>                                                                                                                                                                                                                                                
					<m:Label text="{plantsModel>/plant}"/>                                                                                                                                                                                                                    
				</m:VBox>                                                                                                                                                                                                                                                  
                                                                                                                                                                                                                                                               
				<m:VBox class="sapUiSmallMarginBottom sapUiLargeMarginEnd">                                                                                                                                                                                                
					<m:Title level="H4" titleStyle="H4" text="Property" class="sapUiTinyMarginBottom" />                                                                                                                                                                      
					<m:Label text="{plantsModel>/property}"/>                                                                                                                                                                                                                 
				</m:VBox>                                                                                                                                                                                                                                                  
                                                                                                                                                                                                                                                               
				<m:VBox class="sapUiSmallMarginBottom">                                                                                                                                                                                                                    
					<m:Title level="H4" titleStyle="H4" text="Address" class="sapUiTinyMarginBottom" />                                                                                                                                                                       
					<m:Label wrapping="true" text="{plantsModel>/address}"/>                                                                                                                                                                                                  
				</m:VBox>                                                                                                                                                                                                                                                  
			</m:FlexBox>                                                                                                                                                                                                                                                
		</headerContent>                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                               
		<sections>                                                                                                                                                                                                                                                   
                                                                                                                                                                                                                                                               
			<ObjectPageSection titleUppercase="false" id="personalSection" title="Basic Details">                                                                                                                                                                       
				<subSections>                                                                                                                                                                                                                                              
					<ObjectPageSubSection id="personalSectionSS1" titleUppercase="false">                                                                                                                                                                                     
						<blocks>                                                                                                                                                                                                                                                 
							<f:SimpleForm id="basicDetails" editable="true" layout="ResponsiveGridLayout" labelSpanXL="4" labelSpanL="4" labelSpanM="12" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0" emptySpanL="0" emptySpanM="0" emptySpanS="0" columnsXL="2" columns+
L="2" columnsM="2" singleContainerFullSize="false">                                                                                                                                                                                                            
								<f:content>                                                                                                                                                                                                                                            
									<core:Title text=""/>                                                                                                                                                                                                                                 
									<m:Label  text="{i18n>kioskProp}" />                                                                                                                                                                                                                  
									<m:Select id="kiskProp" enabled="false" valueStateText="Value cannot be blank" valueState="{/KioskProperty}" selectedKey="{parts: ['plantBasicDetailsModel>/KioskProperty'], formatter: '.formatter.getSelectedKeyNo'}">                              
										<m:items>                                                                                                                                                                                                                                            
											<core:Item text="Yes" key="Y" />                                                                                                                                                                                                                    
											<core:Item text="No" key="N"/>                                                                                                                                                                                                                      
                                                                                                                                                                                                                                                               
										</m:items>                                                                                                                                                                                                                                           
									</m:Select>                                                                                                                                                                                                                                           
                                                                                                                                                                                                                                                               
									<m:Label text="{i18n>budPropNumber}" />                                                                                                                                                                                                               
									<m:Input enabled="false" maxLength="7" id="budProp" value="{plantBasicDetailsModel>/BuddyPropertyNumber}" showValueHelp="false" valueHelpRequest="_onValueHelpRequestProperty" valueHelpOnly="true"/>                                                 
									<m:Label text="{i18n>retailSqFootage}" />                                                                                                                                                                                                             
									<m:Input enabled="false" maxLength="5" id="retailStoreSq" value="{plantBasicDetailsModel>/RetailStoreSquareFootage}" />                                                                                                                               
                                                                                                                                                                                                                                                               
									<m:Label text="{i18n>historicalProp}" />                                                                                                                                                                                                              
									<m:Input enabled="false"  id="histProp" value="{plantBasicDetailsModel>/HistoricalProperty}" type="Url" />                                                                                                                                            
									<m:Label text="{i18n>psaProp}" />                                                                                                                                                                                                                     
									<m:Input enabled="false"  id="psaProp" value="{plantBasicDetailsModel>/PsaProperty}" />                                                                                                                                                               
									<m:Label text="{i18n>transForm}" />                                                                                                                                                                                                                   
									<m:Input enabled="false"  id="transF" value="{plantBasicDetailsModel>/TransferFrom}" />                                                                                                                                                               
                                                                                                                                                                                                                                                               
									<core:Title text="" />                                                                                                                                                                                                                                
									<m:Label text="{i18n>propEmailAdd}" />                                                                                                                                                                                                                
									<m:Input enabled="false" id="email"  value="{plantsModel>/Email}" />                                                                                                                                                                                  
                                                                                                                                                                                                                                                               
									<m:Label text="{i18n>kisokActivDate}" />                                                                                                                                                                                                              
									<m:DatePicker id="kisok" value="{&nbsp;                                                                                                                                                                                                               
					parts: ['plantBasicDetailsModel>/KioskActiveDate'], formatter: '.formatter.formatDateTime',                                                                                                                                                               
					type: 'sap.ui.model.type.Date'                                                                                                                                                                                                                            
				  }" valueFormat="yyyy-MM-dd" enabled="false" placeholder="Choose Date.."/>                                                                                                                                                                                
									<m:Label text="{i18n>offSqFoootage}" />                                                                                                                                                                                                               
									<m:Input enabled="false" id="offSq" value="{plantBasicDetailsModel>/OfficeSquareFootage}" />                                                                                                                                                          
									<m:Label text="{i18n>retailSqSize}" />                                                                                                                                                                                                                
									<m:Input enabled="false" id="retailST" value="{plantBasicDetailsModel>/RetailStorageSize}" />                                                                                                                                                         
									<m:Label text="{i18n>histOwner}" />                                                                                                                                                                                                                   
									<m:Input enabled="false"  id="histOwner" value="{plantBasicDetailsModel>/HistoricalOwner}" />                                                                                                                                                         
									<m:Label text="{i18n>psaOwner}" />                                                                                                                                                                                                                    
									<m:Input enabled="false"  id="PsaOwner" value="{plantBasicDetailsModel>/PsaOwner}" />                                                                                                                                                                 
									<m:Label  text="{i18n>geoCode}" />                                                                                                                                                                                                                    
									<m:Input enabled="false"  valueStateText="Value cannot be blank" valueState="{/GeoCode}" id="geo" value="{plantBasicDetailsModel>/GeoCode}" />                                                                                                        
                                                                                                                                                                                                                                                               
								</f:content>                                                                                                                                                                                                                                           
							</f:SimpleForm>                                                                                                                                                                                                                                         
						</blocks>                                                                                                                                                                                                                                                
					</ObjectPageSubSection>                                                                                                                                                                                                                                   
				</subSections>                                                                                                                                                                                                                                             
			</ObjectPageSection>                                                                                                                                                                                                                                        
                                                                                                                                                                                                                                                               
		</sections>                                                                                                                                                                                                                                                  
	</ObjectPageLayout>                                                                                                                                                                                                                                           
</mvc:View>                                                                                                                                                                                                                                                    